<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Amazon Test Suite" parallel="methods" thread-count="2">
    <listeners>
        <listener class-name="com.yehorychev.selenium.listeners.AllureTestListener"/>
    </listeners>
    <test name="Amazon UI Tests">
        <classes>
            <class name="com.yehorychev.selenium.tests.amazon.ui.AmazonTests"/>
        </classes>
    </test>
</suite>

